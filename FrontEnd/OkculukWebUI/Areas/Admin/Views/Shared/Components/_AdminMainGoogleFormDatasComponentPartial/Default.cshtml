@model List<GetAllGoogleFormDto>

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title">Google Form Verileri</h4>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Etkinlik Id</th>
                                <th>Ad</th>
                                <th>Soyad</th>
                                <th>Katılım Durumu</th>
                                <th>Reddetme Sebebi</th>
                                <th>Cevap Tarihi</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item.EventId</td>
                                    <td>@item.Name</td>
                                    <td>@item.Surname</td>
                                    <td>@item.IsJoin</td>
                                    <td>@item.RejectedReason</td>
                                    <td>@item.CreatedDate</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>